/* pages/car/login/login.wxss */
.header {
    margin: 90rpx 0 90rpx 50rpx;
    text-align: center;
    width: 650rpx;
    height: 300rpx;
    line-height: 450rpx;
}

.header image {
    width: 200rpx;
    height: 200rpx;
}

.content {
    margin-left: 50rpx;
    margin-bottom: 90rpx;
}

.content text {
    display: block;
    color: #9d9d9d;
    margin-top: 40rpx;
}

.buttom{
   display: flex;
   justify-content: center;
   align-items: center;
   flex-direction: row;
}
.bottom.enter {
    width: 300rpx;
    height: 50rpx;
    border-radius: 50rpx;
    margin: 70rpx 50rpx;
    font-size: 35rpx;
}
.bottom.refuse {
   width: 300rpx;
    height: 50rpx;
    border-radius: 50rpx;
    margin: 70rpx 50rpx;
    font-size: 35rpx;
    background-color: rgba(95, 107, 95, 0.301);
}